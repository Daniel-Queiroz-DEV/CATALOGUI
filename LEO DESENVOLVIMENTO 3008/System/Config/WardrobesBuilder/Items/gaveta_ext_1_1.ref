    </ResizeItem>
    <ResizeItem Side="Top" Value="-(#ESC_ESP_BAS#+#FOL_PAI#)+#ESC_FOL_MOD_SUP#+(#ESC_ESP_PRAT_FIX#+#ESC_FOL_PAI#)/2-#ESC_FOL_ENT_GAV#/2">
