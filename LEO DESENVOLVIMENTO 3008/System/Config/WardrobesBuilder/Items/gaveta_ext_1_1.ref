<?xml version="1.0" encoding="utf-8"?>
<Item Id="gaveta_ext_1_1" Description="Interna\Nova categoria\Sapateira\Nova categoria\Sapateira\Nova categoria\Externa" EntityId="ESC_CAI_GAV_EXT" GroupName="Gavetas" Name="Gaveta Externa" Type="Drawer">
