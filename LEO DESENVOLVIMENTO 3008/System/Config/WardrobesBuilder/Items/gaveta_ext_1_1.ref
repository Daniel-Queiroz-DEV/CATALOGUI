          <HasNeighbourItem ItemType="Front" Side="Top" />
          <HasNeighbourItem ItemType="Drawer" Side="Top" />
        </Or>
      </Conditions>
    </ResizeItem>
    <ResizeItem Side="Top" Value="(#ESC_ESP_PRAT_FIX#+#FOL_PAI#)-(#ESC_ESP_BAS#+#FOL_PAI#)">
